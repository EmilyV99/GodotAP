[filesystem]

import/blender/enabled=false

[gui]

timers/tooltip_delay_sec=0.2
theme/custom="res://godot_ap/ui/themes/dark_theme.tres"
theme/custom_font="uid://bsf22ltaya6h0"

[rendering]

renderer/rendering_method="gl_compatibility"
